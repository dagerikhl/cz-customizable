{
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "env": {
    "node": true,
    "jasmine": true
  },
  "globals": {
    "console": true,
    "module": true
  },
  "plugins": ["prettier"],
  "rules": {
    "padding-line-between-statements": ["error", { "blankLine": "always", "prev": "*", "next": "return" }],
    "line-comment-position": ["error", { "position": "above" }]
  }
}
